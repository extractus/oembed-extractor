{
  "version": "3.1.2",
  "name": "oembed-parser",
  "description": "Get oEmbed data from given URL.",
  "homepage": "https://www.npmjs.com/package/oembed-parser",
  "repository": {
    "type": "git",
    "url": "git@github.com:ndaidong/oembed-parser.git"
  },
  "author": "@ndaidong",
  "main": "./src/main.js",
  "type": "module",
  "exports": {
    "import": "./src/main.js",
    "require": "./dist/cjs/oembed-parser.js"
  },
  "imports": {
    "cross-fetch": "./src/deno/cross-fetch.js"
  },
  "browser": {
    "cross-fetch": "./src/deno/cross-fetch.js"
  },
  "types": "./index.d.ts",
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "lint": "standard .",
    "lint:fix": "standard --fix",
    "pretest": "npm run lint",
    "test": "NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --verbose --coverage=true",
    "eval": "node eval",
    "eval:cjs": "node cjs-eval",
    "sync": "node sync",
    "build": "node build src/main.js",
    "reset": "node reset"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5"
  },
  "devDependencies": {
    "esbuild": "^0.15.10",
    "jest": "^29.1.2",
    "nock": "^13.2.9",
    "standard": "^17.0.0"
  },
  "standard": {
    "ignore": [
      "/dist",
      "/examples"
    ]
  },
  "keywords": [
    "oembed",
    "extractor",
    "parser",
    "util"
  ],
  "license": "MIT"
}
